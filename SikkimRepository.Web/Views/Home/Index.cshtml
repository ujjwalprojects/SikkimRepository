<!-- Category Start -->
@foreach (var Category in Model.GenComponentModelList.GroupBy(x => x.CategoryName))
{
    <div id="rs-events" class="rs-events sec-spacer-30">
        <div class="container">
            <div class="row rs-vertical-middle">
                <div class="col-lg-12 col-md-12">
                    <div class="sec-title mb-30 text-center">
                        <h2 class="text-uppercase">@Category.Key</h2>
                        <p>Components across the State</p>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12">
                    <div id="accordion" class="rs-accordion-style1">
                        <div class="row">
                            @foreach (var Component in Category.GroupBy(x => x.ComponentID))
                            {
                                <div class="col-md-4">
                                    <div class="card">
                                        <div class="card-header" id="@Component.Key">
                                            <h3 class="acdn-title collapsed" data-toggle="collapse" data-target="#C_@Component.Key" aria-expanded="false" aria-controls="@Component.Key">
                                                @Component.Select(x => x.ComponentName).FirstOrDefault()
                                            </h3>
                                        </div>
                                        <div id="C_@Component.Key" class="collapse" aria-labelledby="@Component.Key" data-parent="#accordion">
                                            <div class="card-body">
                                                <ol>
                                                    @foreach (var subitem in Component.GroupBy(x => x.SubComponentID))
                                                    {
                                                        if (subitem.Count() != 0)
                                                        {
                                                            <li>
                                                                <a href="/home/GetContentBySubComp?CID=@subitem.Select(x=>x.ComponentID).FirstOrDefault()&SCID=@subitem.Key">@subitem.Select(x => x.SubComponentName).FirstOrDefault()</a>
                                                            </li>
                                                        }
                                                        else
                                                        {
                                                            <li>
                                                                No Sub Component Found.
                                                            </li>
                                                        }
                                                    }
                                                </ol>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<!-- Category End -->
<!-- Latest Video Start -->
<div id="rs-latest-news" class="rs-latest-news  sec-spacer-50">
    <div class="container">
        <div class="row rs-vertical-middle">
            <div class="col-lg-4 col-md-12">
                <div class="sec-title mb-30">
                    <h2>VIDEOS</h2>
                    <p>View Latest Videos</p>
                </div>
                <div class="">
                    <div class="rs-products">
                        <div class="view-btn mt-1 text-center float-left">
                            <a href="/videos/index">VIEW ALL</a>
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-lg-8 col-md-12">
                <div class="row">
                    <div class="col-md-9">
                        <div class="latest-news-slider">
                            @foreach (var item in Model.GenVideos)
                            {
                                if (@item.VideoFilePathPublished != null)
                                {
                                    string[] vArr = item.VideoFilePathPublished.Split('?');
                                    string vLink = "https://www.youtube.com/embed/" + vArr[1].Substring(2);
                                    <div>
                                        <div class="news-normal-block">
                                            <div class="news-img">
                                                <iframe style="width: 100%;height: 328px; display:block" src="@vLink"></iframe>
                                            </div>
                                            <div class="news-date">
                                                <i class="fa fa-calendar-check-o"></i>
