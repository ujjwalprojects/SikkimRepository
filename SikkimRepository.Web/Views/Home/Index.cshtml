@model SikkimRepository.Entities.CustomModels.GenCustomModel
@{
    ViewBag.Title = "Home Page";
}
@{
    List<SikkimRepository.Entities.Models.utblBanner> banners = new List<SikkimRepository.Entities.Models.utblBanner>();
    int bannercount = 0;
    if (ViewBag.Banner != null)
    {
        banners = (List<SikkimRepository.Entities.Models.utblBanner>)ViewBag.Banner;
        bannercount = banners.Count();
    }
}
@*@{
        SikkimRepository.Entities.DataAccess.dalSubComponent ObjSubComp = new SikkimRepository.Entities.DataAccess.dalSubComponent();
    }*@
<style>
    svg {
        height: 26vw;
    }

    path {
        fill: #97c8f0;
        transition: .6s fill;
        stroke-width: 1;
    }

    g {
        stroke: #e2e2e2;
    }

    path:hover {
        fill: #b9daf6;
    }

    .text-ellipsis {
        display: block;
        text-overflow: ellipsis;
        width: 100%;
        overflow: hidden;
        white-space: nowrap;
    }

    .text-ellipsis-width {
        width: 80%;
    }

    .card-body > ol > li a {
        color: #000000;
    }

    .product-img a {
        color: #f9f9f9;
    }

    .mmd-card {
        background: #f9f9f9;
    }

    .rs-events:nth-child(odd) {
        background: #f9f9f9;
    }
</style>
<!-- Slider banner Start -->
<div id="rs-slider" class="slider-section4 slider-overlay-2">
    <div id="home-slider">
        @for (int i = 0; i < bannercount; i++)
        {
            if (i == 0)
            {
                <!-- Item 1 -->
                <div class="item active">
                    <img src="@banners.ElementAt(i).BannerPath" />
                    <div class="slide-content">
                        <div class="display-table">
                            <div class="display-table-cell">
                                <div class="container">
                                    <h1 class="slider-title" data-animation-in="fadeInLeft" data-animation-out="animate-out">@banners.ElementAt(i).BannerTitle</h1>
                                    <p data-animation-in="fadeInUp" data-animation-out="animate-out" class="slider-desc">@banners.ElementAt(i).BannerDescription</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
            else
            {
                <!-- Item 2 -->
                <div class="item">
                    <img src="@banners.ElementAt(i).BannerPath" />
                    <div class="slide-content">
                        <div class="display-table">
                            <div class="display-table-cell">
                                <div class="container">
                                    <h1 class="slider-title" data-animation-in="fadeInUp" data-animation-out="animate-out">@banners.ElementAt(i).BannerTitle</h1>
                                    <p data-animation-in="fadeInUp" data-animation-out="animate-out" class="slider-desc">@banners.ElementAt(i).BannerDescription</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        }
        <!-- Item 3 -->
